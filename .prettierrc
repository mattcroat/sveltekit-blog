{
	"semi": false,
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"printWidth": 80,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-css-order"],
	"pluginSearchDirs": ["."],
	"overrides": [
		{
			"files": "*.svelte",
			"options": { "parser": "svelte" }
		}
	],
	"order": "smacss"
}
